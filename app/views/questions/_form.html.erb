<%= form_with model:@question, local: true do |form| %>
    <% if @question.errors.present? %>
    <%= @question.errors.full_messages.join(', ')%>
    <% end %>
    <div>
        <%= form.label :title  %> 
        <%= form.text_field :title  %> 
    </div>
    <div>
        <%= form.label :body  %>
        <%= form.text_area :body, cols:50 ,rows:5  %>  
    </div>
    <%=form.submit %> 
    
    <% end %>
    